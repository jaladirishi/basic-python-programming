#arithmetic operator
a = int(input("Enter value of a: "))
b = int(input("Enter value of b: "))
operator = input("Enter operator (+, -, *, /, %): ")

if operator == "+":
    print(a + b)
elif operator == "-":
    print(a - b)
elif operator == "*":
    print(a * b)
elif operator == "/":
    if b != 0:
        print(a / b)
    else:
        print("Invalid operation (division by zero)")
elif operator == "%":
    if b != 0:
        print(a % b)
    else:
        print("Invalid operation (modulus by zero)")
else:
    print("Invalid operator")



#Non-Primitive Data Types
List1 = [1, 2, 3, 4]
tuple1 = (10, 20, 30)
set1 = {2, 1, 6, 3, 4}
Dict1 = {'name': 'rishi', 'age': 20, 'city': 'Vijayawada'}

print("List is:", List1)
print("tuple is:", tuple1)
print("set is:", set1)
print("Dictionary is:", Dict1)



#logical operators
a = 10
b = 20

if (a > 0 and b > 0):
    print("True for and command")
elif (a > 0 or b < 0):
    print("True for or command")
elif (not a == 0):
    print("True for not command")
else:
    print("No condition matched")


