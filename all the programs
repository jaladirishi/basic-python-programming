#marks
marks = int(input("Enter the marks"))
if marks >= 90:
    print("A: Excellent")
elif marks >= 70 and marks < 90:
    print("B: Good")
elif marks >= 50 and marks < 70:
    print("C")
else:
    print("Fail")

#sum of cubes
def sum_of_cubes(m, n):
    total = 0
    if m > n:
        print("Invalid range")
        return 0
    for i in range(m, n+1):
        total = total + i**3
    return total

m1 = int(input("Enter the value"))
m2 = int(input("Enter the value"))
print("Sum of cubes:", sum_of_cubes(m1, m2))

